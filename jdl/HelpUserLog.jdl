// 帮助用户日志
entity HelpUserLog(kb_help_user_log) {
    
    // 服务器ID
    guildId String
    
    // 频道ID
    channelId String
    
    // 被帮助用户ID
    userId String

    // 帮助用户ID
    helpUserId String
    
    // 加入时间
    joinAt Instant
    
    // 退出时间
    exitAt Instant

    // 状态
    status String
    
    createdBy String
    createdDate Instant
    lastModifiedBy String
    lastModifiedDate Instant
}

dto * with mapstruct
service * with serviceClass
filter *
