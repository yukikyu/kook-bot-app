// 帮助统计
entity HelpStatistics
  (
  kba_help_statistics
  ) {
  // 服务器ID
  guildId String

  // 帮助用户ID
  helpUserId String

  // 时长(分钟)
  duration Integer

  // 月份
  month Instant

  createdBy String
  createdDate Instant
  lastModifiedBy String
  lastModifiedDate Instant
}

